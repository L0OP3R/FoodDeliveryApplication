package example;

import java.util.ArrayList;
import java.util.List;

public class ListExample {
	ArrayList<String> names = new ArrayList<>();
	
	public static void main(String[] args) {}
	
	//method overloading
	//method should be in same class and method name should be same and same return type but parameters are different
	public int add(int a, int b) {
		return 0;
		
	}
	public int add(int a , int b, int c) {
		return 0;
		
	}
	public int add(float c , double b, double a) {
		return 0;
		
	}
	//example of changing the return type so this method does not come under overloading
	/*
	 * public double add(int a, int b) {
	 * 
	 * }
	 */
	//Method Overriding

}
	


